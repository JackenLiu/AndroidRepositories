        <activity android:name=".dialog.AlertDialogActivity" />
        <activity android:name=".menu.PopupMenuActivity" />
        <activity android:name=".menu.ContextMenuActivity" />
        <activity android:name=".menu.OptionMenuActivity" />
        <activity android:name=".layout.ConstraintLayoutActivity" />
        <activity android:name=".layout.GridLayoutActivity" />
        <activity android:name=".layout.TableLayoutActivity" />
        <activity android:name=".asynctask.DownloadActivity" />
        <activity android:name=".asynctask.AsyncTaskActivity" />
        <activity android:name=".handler.HandlerActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>